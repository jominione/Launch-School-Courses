// Add up all the ages from the Munster family object:

let ages = {
  Herman: 32,
  Lily: 30,
  Grandpa: 5843,
  Eddie: 10,
  Marilyn: 22,
  Spot: 237
};

// Solution:

let sum = 0;
let values = Object.values(ages);

values.forEach( val => sum += val)

console.log(sum);

// Solution using reduce method:

let result = values.reduce((agesSum, currAge) => agesSum + currAge, 0);
console.log(result);

// Discussion: The advantage of the reduce method is that you don't have to
// declare and initialise a variable and then reassign to that value from
// inside the callback.